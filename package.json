    "classnames": "^2.2.6",
    "cookie": "^0.4.0",
    "express": "^4.16.4",
    "formik": "^1.5.1",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
